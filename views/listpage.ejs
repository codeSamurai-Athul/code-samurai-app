<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>book</th><th>authour</th><th>price</th><th>link</th>
            <th>AddedBy</th>
        </tr>

        <% items.forEach(function(item){  %>

<tr>
<td><a href="<%= item.url%>" target="opensim"><%= item.bookname %></a></td>
    <td><%= item.authour %></td>
    <td><%= item.price %></td>
    <td><%= item.link%></td>
    <td><%= item.addedBy%></td>

</tr>
        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
